{
  "name": "tracemon",
  "version": "1.0.0",
  "description": "Visualize network topology generated by Traceroutes from [RIPE Atlas](https://atlas.ripe.net)",
  "scripts": {
    "build": "bash -e build.sh",
    "build:jquery-libs": "r.js -o builders/jquery-libs.build.js",
    "build:jquery-libs-amd": "r.js -o builders/jquery-libs-amd.build.js",
    "build:style-lib": "lessc .tmp/view/css/style-lib.less .tmp/view/css/style-lib.css && minify .tmp/view/css/style-lib.css > .tmp/view/css/style-lib.min.css",
    "build:tracemon": "r.js -o builders/tracemon.build.js",
    "examples": "([ -d \"dist\" ] && http-server -o examples) || echo \"Error. No build in /dist found.\nUse \\\"npm run build\\\".\" && exit 1",
    "test": "echo No tests defined && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robin-rpr/tracemon.git"
  },
  "author": "see ./AUTHORS",
  "license": "BSD-3",
  "bugs": {
    "url": "https://github.com/robin-rpr/tracemon/issues"
  },
  "homepage": "https://github.com/robin-rpr/tracemon#readme",
  "devDependencies": {
    "http-server": "^14.1.1",
    "less": "^4.1.3",
    "minify": "^9.1.0",
    "requirejs": "^2.1.15"
  }
}
